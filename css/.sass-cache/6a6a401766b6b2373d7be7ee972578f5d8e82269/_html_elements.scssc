.accordion {
	border: 1px solid $mid_grey;
	
	.title {
		padding: 20px;
		line-height: 20px;
		position: relative;
		cursor: pointer;
		
		.arrow {
			margin: 0 0 0 5px;
			background-position: -475px -67px;
