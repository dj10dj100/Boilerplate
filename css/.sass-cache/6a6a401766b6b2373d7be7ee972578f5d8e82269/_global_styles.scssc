:@template"≥/* General Styles */

body {
	font: $font_stack;
	font-size:13px;
	color: $black;
}
.right {
	float:right;
}
.left {
	float:left;
}
.clear-right {
	clear: right;
}
.clear-left {
	clear: left;
}

.centred {
	text-align: center;
}

.animate {
	-webkit-transition: all 300ms cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	   -moz-transition: all 300ms cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	    -ms-transition: all 300ms cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	     -o-transition: all 300ms cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	        transition: all 300ms cubic-bezier(0.455, 0.030, 0.515, 0.955);
	
	-webkit-transition-timing-function: cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	   -moz-transition-timing-function: cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	    -ms-transition-timing-function: cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	     -o-transition-timing-function: cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	        transition-timing-function: cubic-bezier(0.455, 0.030, 0.515, 0.955);
}

/* Buttons */

.button {
	height:40px;
	color: $white;
	font-size: 16px;
	font-weight:500;
	line-height:40px;
	padding:0 18px;
	display:inline-block;
	background-position: 0px -578px;
	cursor:pointer;
	
	-webkit-transition: all 300ms cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	   -moz-transition: all 300ms cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	    -ms-transition: all 300ms cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	     -o-transition: all 300ms cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	        transition: all 300ms cubic-bezier(0.455, 0.030, 0.515, 0.955);
	
	-webkit-transition-timing-function: cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	   -moz-transition-timing-function: cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	    -ms-transition-timing-function: cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	     -o-transition-timing-function: cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	        transition-timing-function: cubic-bezier(0.455, 0.030, 0.515, 0.955);
}

.button-large {
	height: 40px;
	color: $white;
	font-size: 16px;
	font-weight: 200;
	line-height: 18px;
	padding: 10px 0 25px 0;
	display: block;
	float: left;
	background-position: 0px -570px;
	cursor: pointer;
	text-align: center;
	margin-right: 10px;
	width: 130px;
	
	-webkit-transition: all 300ms cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	   -moz-transition: all 300ms cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	    -ms-transition: all 300ms cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	     -o-transition: all 300ms cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	        transition: all 300ms cubic-bezier(0.455, 0.030, 0.515, 0.955);
	
	-webkit-transition-timing-function: cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	   -moz-transition-timing-function: cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	    -ms-transition-timing-function: cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	     -o-transition-timing-function: cubic-bezier(0.455, 0.030, 0.515, 0.955); 
	        transition-timing-function: cubic-bezier(0.455, 0.030, 0.515, 0.955);
}

.orange-button-large {
	@extend .button-large;
	background-color: $orange;
}

.orange-button {
	@extend .button;
	background-color: $orange;
	
	span.sprite.arrow,
	span.sprite.blue-right-arrow-small {
		margin: 0 0 0 5px;
		background-position: -145px -67px;
		display: inline-block;
		width: 6px;
		height: 13px;
	}
}

.grey-button{
	@extend .button;
	background-color: $black;
	
}
.blue-button {
	@extend .button;
	background-color: $blue;
	color: $white;
		
	span.sprite.arrow {
		margin: 0 0 0 5px;
		background-position: -475px -67px;
		display: inline-block;
		width: 6px;
		height: 13px;
	}
}

.black-button {
	@extend .button;
	background-color: $black;
	color: $white;
	background-position: -770px -578px;
		
	span.sprite.arrow {
		margin: 0 0 0 5px;
		background-position: -475px -85px;
		display: inline-block;
		width: 6px;
		height: 9px;
	}
}

.button:hover {
	background-position-y: -588px;
}

.button-large:hover {
	background-position-y: -580px;
	text-decoration: none;
}

.lt-ie9{
	div,
	span,
	.row,
	.container{
	}
}

.content {
	border-bottom: 1px solid $light_grey;
	width: 620px;
	
	p {
		padding-right: 0;
	}

	&.no-border{
		border:none;
	}
	&.padded-right{
		padding-right:10%;
		width:90%;
	}
}

.blockquote {
	padding: 30px;
	margin-bottom: 20px;
	clear: both;
	
	p {
		width: 100%;
		padding: 0;
	}
	
	h2 {
		margin-top: 0;
	}
}

.content.last {
	border-bottom: none;
}

.listing {
	list-style: none;
	
	.article {
		padding: 30px 50px 30px 20px;
		border-bottom: 1px solid $light_grey;
		
		.excerpt {
			/*margin-top: 10px;*/
		}
	}
	
	.article.last {
		border-bottom: none;
	}
}

.six-sixty .row .six-sixty.news-article-extras {
	width: 620px;
	
	.three-hundred {
		width: 288px;
		margin-right: 20px;
	}
}

.link-group {
	position: relative;
	margin-top: 30px;
	margin-bottom: 30px;
	
	.links-row {
		list-style: none;
		margin: 0;
		padding: 0;
		overflow: hidden;
		
		li {
			display: block;
			float: left;
			
			a,
			.link-current {
				display: block;
				padding: 15px 10px 15px 20px;
			}
		}
	}
}

.anchor-links {
	@extend .link-group;
	
	border-top: 1px solid $blue;
	background-image: url(../img/grey-shadow-grad.png);
	background-position: top center;
	background-repeat: repeat-x;
}

.pagination {
	@extend .link-group;
	
	.link-prev,
	.link-next {
		display: block;
		padding: 15px 20px 15px 20px;
		position: absolute;
	}
	
	.link-prev {
		top: 0px;
		left: 0px;
	}
	
	.link-next {
		top: 0px;
		right: 0px;
	}
	
	.links-row {
		width: 140px;
		margin: 0 auto;
		
		li {
			a,
			.link-current {
				display: block;
				padding: 15px 5px 15px 5px;
			}
		}
	}
}

.directory {
	@extend .link-group;
	
	.links-row {
		padding: 10px 15px;
		
		li {
			a,
			.link-current {
				padding: 15px 5px 15px 5px;
			}
			.strong {
				padding-right: 15px;
			}
		}
	}
}


.date-group {

	.date-anchor {
		position: relative;
		left: -10px;
		width: 670px;
		
		.date-bar {
			padding: 15px 20px;
			margin-bottom: 0;
			position: relative;
			
			.top-scroll {
				float: right;
			}
		}
		
		.small-blue-corner-bottom-right {
			position: absolute;
			top: -6px;
			left: 0px;
		}
	}
	
	.article {
		padding-top: 20px;
		padding-bottom: 20px;
	}
	
	.article.last {
		padding-bottom: 30px;
	}
}

.slider{

	position:relative;
	padding:0;
	height:auto;

	.slider-image{
		width:100%;
		height:410px;
		height:auto;
		position: relative;
	}
	
	.text-area {
		width:800px;
		height:410px;
		background: url(../img/dark-grey-decal.png) top left no-repeat;
		position:absolute;
		top:0;
		left:0;
	}

	.copy{
		position:absolute;
		background-image: url(../img/dark-grey-decal.jpg); 
		bottom:50px;
		left:20px;
		width:400px;

		p,h5{
			padding:0;
			margin:0;
		}	
	}
}

.site-map{

	ul{

		li{

			list-style-type: none;
			margin-left:30px;

			&.main-page{

				margin-left:0;
				font-size: 14px;

				a{
					font-weight:600;
				}
				
			}

			&.sub-page{
				margin-left: 60px;
			}

			a{
				color:$dark_blue;
			}


		}/* li */

	}/* ul */

}

.linkedin{
	position:relative;
	height:790px;

	.more-reasons{
		position:absolute;
		bottom:20px;
		width:343px;
	}
	.reasons-image{
		position:relative;
		height:250px;
		width:100%;

		.copy{

			position:absolute;
			bottom:0;
			width:250px;

			p{
				width:auto;
				line-height:16px;
				font-size:14px;
				padding:10px 10px 10px 10px;
				margin:0;

			}

		}/* copy */

		.logo{
			position:absolute;
			bottom:0;
			right:0;
			width:100px;
			height:68px;
			background:$white;
		}
	}

}/* Linkedin */
.linked-btn{

	display:inline-block;
	height:50px;
	width:auto;
	background:$dark_blue;
	color:$white;
	text-decoration:none;
	line-height: 50px;
	font-size: 16px;

	.sprite{
		float:left;
		margin:16px 15px 15px 15px;
	}
	.copy{
		line-height:50px;
		color:$white;
		font-size:16px;
		float:left;
		width:inherit;
		padding-right:20px;
		padding-left:20px;
		background:$blue;
	}

}
.img-absolute{
	position:absolute;
}
/* Ajax loader -- */
.ajax-loader{
	width: 35px;
	height: 35px;
	margin: 50px auto;
	background: url(../img/ajax-loader.gif) no-repeat; 
	display:none;
