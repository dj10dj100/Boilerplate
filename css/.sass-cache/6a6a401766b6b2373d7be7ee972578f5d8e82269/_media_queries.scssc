			display: block;
			height:auto;
		}
	
		.footer-lists, .social-icons {
			display: none;
		}
	
		.row {
			width:100%;
			margin-bottom: 1px;
		}
	
		h5 {
			font-size: 13px;
			font-weight: 700;
			margin:15px 0;
			display: block;
			text-align: center;
			width: 100%;
			padding: 0;
		}

		.col{
			width:100%;
			padding-left: 0;
		}
		.col-nine{
			padding-bottom:20px;
		}
		
		.search {
			margin: 10px auto;
			
			input {
				-webkit-appearance: none;
				border-radius:0;
				-webkit-box-shadow: inset 1px 1px 4px rgba(102, 102, 102, 0.5);
 				-moz-box-shadow: inset 1px 1px 4px rgba(102, 102, 102, 0.5);
  				box-shadow: inset 1px 1px 4px rgba(102, 102, 102, 0.5);
  				border:none;
			}
		}

	}
	
	.sub-footer{
		background: $light-grey;
		padding: 15px 0;
	
		li{
			width:50%;
			
			a {
				line-height: 25px;
			}
		}
	}
}
