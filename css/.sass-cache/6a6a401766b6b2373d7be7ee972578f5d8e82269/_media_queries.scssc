							}
						}
						
						.fakeh4 {
							margin-bottom: 8px;
						}
						
						input {
							margin-bottom: 8px;
							border-radius: 0;
							border: 1px solid #ffffff;
							-webkit-appearance: none;
						}
					}
					
					&.user-logged-in  {
						a {
							background-color: $black;
						}
						.level-one {
							background-color: $black;
						}
					}
					
				}
				
				li.user-logged-in.open {
					background-color: $black;
					
					a {
						background-color: $black;
					}
					
					.level-one {
						background-color: $black;
